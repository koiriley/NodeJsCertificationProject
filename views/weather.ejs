                          <span class="min"><%=result.list[i].temp.min%>°F</span>
                    <h4 class="card-title"><%=result.list[i].weather[0].main%></h4>
                    <p class="card-text">
                        <p class="day"> Friday </p>
                        <p class="day"> Apr. 23, 2020 </p>
                        
                        <p>Humidity: <%=result.list[i].humidity%>%</p>
                        
                    </p>
                </div>
            </div>
            <%}%>

              <!-- Day 4 Weather Card -->
          <%for(let i=3; i<4; i++){%>
            <div class="card col-md-2">
                <div class="row">
                <img class="card-img-top" src='https://openweathermap.org/img/w/<%=result.list[i].weather[0].icon%>.png' alt="weather"/>
                <span class="topTemp"><%=result.list[i].temp.day%>°F</span>
                </div>
                <div class="card-body">
                <span class="max"><%=result.list[i].temp.max%></span>   /
                          <span class="min"><%=result.list[i].temp.min%>°F</span>
                    <h4 class="card-title"><%=result.list[i].weather[0].main%></h4>
                    <p class="card-text">
                        <p class="day"> Saturday </p>
                        <p class="day"> Apr. 24, 2020 </p>
                        <p>Humidity: <%=result.list[i].humidity%>%</p>
                        
                    </p>
                </div>
            </div>
            <%}%>

              <!-- Day 5 Weather Card -->
          <%for(let i=4; i<5; i++){%>
            <div class="card col-md-2">
                <div class="row">
                <img class="card-img-top" src='https://openweathermap.org/img/w/<%=result.list[i].weather[0].icon%>.png' alt="weather"/>
                <span class="topTemp"><%=result.list[i].temp.day%>°F</span>
                </div>
                <div class="card-body">
                <span class="max"><%=result.list[i].temp.max%></span>   /
                          <span class="min"><%=result.list[i].temp.min%>°F</span>
                    <h4 class="card-title"><%=result.list[i].weather[0].main%></h4>
                    <p class="card-text">
                        <p class="day"> Sunday </p>
                        <p class="day"> Apr. 25, 2020 </p>
                        <p>Humidity: <%=result.list[i].humidity%>%</p>
                        
                    </p>
                </div>
            </div>
            <%}%>

                    </div>
                    <hr/>
        </div>      

        <hr/>
        <!-- Footer -->
  <footer class="page-footer font-small blue pt-4">
      <!-- Footer Links -->
      <div class="container-fluid text-center text-md-left">
        <!-- Grid row -->
        <div class="row">
          <!-- Grid column -->
          <div class="col-md-6 mt-md-0 mt-3">
